{
    "name": "sese-core",
    "version-string": "2.0.0",
    "supports": "x64",
    "license": "Apache-2.0",
    "dependencies": [
        {
            "name": "asio",
            "features": [
                "openssl"
            ],
            "version>=": "1.29.0"
        },
        {
            "name": "libarchive",
            "version>=": "3.7.2"
        },
        {
            "name": "openssl",
            "version>=": "3.2.0#1"
        },
        {
            "name": "zlib",
            "version>=": "1.3"
        },
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        }
    ],
    "default-features": [
        "sqlite3"
    ],
    "features": {
        "tests": {
            "description": "build the unit test",
            "dependencies": [
                {
                    "name": "gtest",
                    "version>=": "1.14.0"
                }
            ]
        },
        "async-logger": {
            "description": "use the async logger",
            "dependencies": []
        },
        "mysql": {
            "description": "add mysql and mariadb support",
            "dependencies": [
                {
                    "name": "libmariadb",
                    "version>=": "3.3.1#3"
                }
            ]
        },
        "sqlite3": {
            "description": "add sqlite3 support",
            "dependencies": [
                {
                    "name": "sqlite3",
                    "version>=": "3.43.2"
                }
            ]
        },
        "psql": {
            "description": "add postgresql support",
            "dependencies": [
                {
                    "name": "libpq",
                    "version>=": "16.0#1"
                }
            ]
        }
    }
}